package tp1.ej7;

import java.util.ArrayList;

public class InvertirLista {
	public void Inversi√≥n (ArrayList<Integer> lista) {
		Integer inicio, actual;
		
		for(int i = 0; i < (lista.size() / 2); i++) {
			inicio = lista.get(i);
			actual = lista.get(lista.size() - 1 - i);
			lista.set(i, actual);
			lista.set(lista.size() - 1 - i, inicio);
		}
	}
	
}
